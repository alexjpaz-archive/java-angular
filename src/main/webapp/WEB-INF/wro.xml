    <group name="app">
        <js minimize="false">/assets/config/init.js</js>
        <js minimize="false">/assets/config/routes*.js</js>
        <js minimize="false">/assets/config/navigation.js</js>
        <js minimize="false">/assets/components/**.js</js>
        <js minimize="false">/assets/screen/**.js</js>
        <js minimize="false">/assets/directives/**.js</js>
        <js minimize="false">/assets/helpers/**.js</js>
        <js minimize="false">/assets/config/bootstrap.js</js>

        <css minimize="false">/assets/css/common/**.css</css>
        <css minimize="false">/assets/components/**.css</css>
        <css minimize="false">/assets/components/**.less</css>
        <css minimize="false">/assets/screen/**.css</css>
        <css minimize="false">/assets/screen/**.less</css>
    </group>

